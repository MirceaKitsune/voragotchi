				"hype":{ "value":"0.0", "per_second":"-0.015", "per_variable":{ "$food":"-0.02" } },
				"max_food":{ "value":"$food", "per_second":"-0.00001" },
				"max_hype":{ "value":"$hype", "per_second":"-0.00001" },
				"health":{ "value":"1", "per_variable":{ "$food":"0.0005", "$temperature":"0.00025" } },
