		"pet_default_baby_interact":[
			{
				"actions":[
					{ "trigger":"load", "action":"pet_control = " },
					{ "trigger":"interval", "conditional":"actions_button_apple == actions_default_button_apple_selected", "action":"pet_interact = pet_default_baby_interact_apple" }
				]
			}
		],
		"pet_default_baby_interact_apple":[
			{
				"layer":{ "geometry":"45% 55% 15% 15% 2", "shape":"100%", "alpha":"0", "color":"#ffffff", "cursor":"url(data/default_pet/cursor_food_apple.gif), auto" },
				"actions":[
					{ "trigger":"interval", "conditional":"actions_button_apple != actions_default_button_apple_selected", "action":"pet_interact = pet_default_baby_interact" },
					{ "trigger":"interval", "conditional":"$hype >= 0.00 && $hype <= 0.25", "action":"$hype += 0.00125" },
					{ "trigger":"interval", "conditional":"$hype >= 0.25 && $hype <= 0.50", "action":"$hype += 0.0025" },
					{ "trigger":"interval", "conditional":"$hype >= 0.50 && $hype <= 0.75", "action":"$hype += 0.00375" },
					{ "trigger":"interval", "conditional":"$hype >= 0.75 && $hype <= 1.00", "action":"$hype += 0.005" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.00 && $hype <= 0.25", "action":"$hype += 0.0025" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.25 && $hype <= 0.50", "action":"$hype += 0.00375" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.50 && $hype <= 0.75", "action":"$hype += 0.005" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.75 && $hype <= 1.00", "action":"$hype += 0.00625" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.00 && $hype <= 0.25 && 0.1 > $random", "action":"pet_chat = pet_default_baby_chat_apple_hype_0" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.25 && $hype <= 0.50 && 0.1 > $random", "action":"pet_chat = pet_default_baby_chat_apple_hype_1" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.50 && $hype <= 0.75 && 0.1 > $random", "action":"pet_chat = pet_default_baby_chat_apple_hype_2" },
					{ "trigger":"hover_start", "conditional":"$hype >= 0.75 && $hype <= 1.00 && 0.1 > $random", "action":"pet_chat = pet_default_baby_chat_apple_hype_3" },
					{ "trigger":"click", "conditional":"$hype >= 0.25", "action":"pet_interact = pet_default_baby_interact_apple_eat" }
				]
			}
		],
		"pet_default_baby_interact_apple_eat":[
			{
				"actions":[
					{ "trigger":"load", "action":"pet_control = pet_default_baby_control_apple_eat" },
					{ "trigger":"load", "conditional":"0.5 > $random", "action":"pet_chat = pet_default_baby_chat_apple_eat" },
					{ "trigger":"load", "delay":"1", "action":"$food += 0.1" },
					{ "trigger":"load", "delay":"1", "action":"$stock_apple -= 0.01" },
					{ "trigger":"load", "delay":"1", "action":"pet_interact = pet_default_baby_interact" }
				]
			}
		],
		"pet_default_baby_control":[
			{
				"actions":[
					{ "trigger":"load", "conditional":"0.1 > $random", "action":"environment_geometries_pet = " },
					{ "trigger":"interval", "conditional":"$hype >= 0.00 && $hype <= 0.25 && $food >= 0.00 && $food <= 0.25", "action":"pet_background = pet_default_baby_background" },
					{ "trigger":"interval", "conditional":"$hype >= 0.00 && $hype <= 0.25 && $food >= 0.25 && $food <= 0.50", "action":"pet_background = pet_default_baby_background_belly_1" },
					{ "trigger":"interval", "conditional":"$hype >= 0.00 && $hype <= 0.25 && $food >= 0.50 && $food <= 0.75", "action":"pet_background = pet_default_baby_background_belly_2" },
					{ "trigger":"interval", "conditional":"$hype >= 0.00 && $hype <= 0.25 && $food >= 0.75 && $food <= 1.00", "action":"pet_background = pet_default_baby_background_belly_3" },
					{ "trigger":"interval", "conditional":"$hype >= 0.25 && $hype <= 0.50 && $food >= 0.00 && $food <= 0.25", "action":"pet_background = pet_default_baby_background_1" },
					{ "trigger":"interval", "conditional":"$hype >= 0.25 && $hype <= 0.50 && $food >= 0.25 && $food <= 0.50", "action":"pet_background = pet_default_baby_background_1_belly_1" },
					{ "trigger":"interval", "conditional":"$hype >= 0.25 && $hype <= 0.50 && $food >= 0.50 && $food <= 0.75", "action":"pet_background = pet_default_baby_background_1_belly_2" },
					{ "trigger":"interval", "conditional":"$hype >= 0.25 && $hype <= 0.50 && $food >= 0.75 && $food <= 1.00", "action":"pet_background = pet_default_baby_background_1_belly_3" },
					{ "trigger":"interval", "conditional":"$hype >= 0.50 && $hype <= 0.75 && $food >= 0.00 && $food <= 0.25", "action":"pet_background = pet_default_baby_background_2" },
					{ "trigger":"interval", "conditional":"$hype >= 0.50 && $hype <= 0.75 && $food >= 0.25 && $food <= 0.50", "action":"pet_background = pet_default_baby_background_2_belly_1" },
					{ "trigger":"interval", "conditional":"$hype >= 0.50 && $hype <= 0.75 && $food >= 0.50 && $food <= 0.75", "action":"pet_background = pet_default_baby_background_2_belly_2" },
					{ "trigger":"interval", "conditional":"$hype >= 0.50 && $hype <= 0.75 && $food >= 0.75 && $food <= 1.00", "action":"pet_background = pet_default_baby_background_2_belly_3" },
					{ "trigger":"interval", "conditional":"$hype >= 0.75 && $hype <= 1.00 && $food >= 0.00 && $food <= 0.25", "action":"pet_background = pet_default_baby_background_3" },
					{ "trigger":"interval", "conditional":"$hype >= 0.75 && $hype <= 1.00 && $food >= 0.25 && $food <= 0.50", "action":"pet_background = pet_default_baby_background_3_belly_1" },
					{ "trigger":"interval", "conditional":"$hype >= 0.75 && $hype <= 1.00 && $food >= 0.50 && $food <= 0.75", "action":"pet_background = pet_default_baby_background_3_belly_2" },
					{ "trigger":"interval", "conditional":"$hype >= 0.75 && $hype <= 1.00 && $food >= 0.75 && $food <= 1.00", "action":"pet_background = pet_default_baby_background_3_belly_3" }
				]
			}
		],
		"pet_default_baby_control_apple_eat":[
			{
				"actions":[
					{ "trigger":"interval", "conditional":"$food >= 0.00 && $food <= 0.25", "action":"pet_background = pet_default_baby_background_apple_eat" },
					{ "trigger":"interval", "conditional":"$food >= 0.25 && $food <= 0.50", "action":"pet_background = pet_default_baby_background_apple_eat_belly_1" },
					{ "trigger":"interval", "conditional":"$food >= 0.50 && $food <= 0.75", "action":"pet_background = pet_default_baby_background_apple_eat_belly_2" },
					{ "trigger":"interval", "conditional":"$food >= 0.75 && $food <= 1.00", "action":"pet_background = pet_default_baby_background_apple_eat_belly_3" }
				]
			}
		],
