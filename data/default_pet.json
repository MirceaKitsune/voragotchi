			{
				"layer":{ "geometry":"0% 0% 5% 5% 1", "shape":"100%", "alpha":"1.0", "gradient":{ "value":"$health", "color1":"#aa0000", "color2":"#110000", "direction":"top" } },
				"text":{ "content":"+\n$health$100", "align":"center", "color":"#ffffff", "size":"75%" }
			},
			{
				"layer":{ "geometry":"5% 0% 90% 2.5% 1", "shape":"10px", "alpha":"1.0", "gradient":{ "value":"$food", "color1":"#aaaa00", "color2":"#111100", "direction":"right" } },
				"text":{ "content":"Food: $food$100%", "align":"center", "color":"#ffffff", "size":"75%" }
			},
			{
				"layer":{ "geometry":"5% 2.5% 90% 2.5% 1", "shape":"10px", "alpha":"1.0", "gradient":{ "value":"$hype", "color1":"#00aa00", "color2":"#001100", "direction":"right" } },
				"text":{ "content":"Excitement: $hype$100%", "align":"center", "color":"#ffffff", "size":"75%" }
			}
